{
  "name": "@innoai-tech/config",
  "version": "0.2.4",
  "monobundle": {
    "env": "browser",
    "exports": {
      ".": "./src/index.ts",
      "./loader": "./src/loader/index.ts",
      "./vite-plugin-inject-config": "./src/vite-plugin-inject-config/index.ts"
    }
  },
  "dependencies": {
    "@babel/runtime-corejs3": "*",
    "@innoai-tech/lodash": "workspace:^*",
    "esbuild": "^0.14.42",
    "vite": "^2.9.9"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "bin": {},
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    },
    "./loader": {
      "import": {
        "types": "./loader.d.ts",
        "default": "./loader.mjs"
      }
    },
    "./vite-plugin-inject-config": {
      "import": {
        "types": "./vite-plugin-inject-config.d.ts",
        "default": "./vite-plugin-inject-config.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "scripts": {
    "prepublishOnly": "../../../node_modules/.bin/monobundle"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:innoai-tech/runtime.git",
    "directory": "nodepkg/@innoai-tech/config"
  },
  "type": "module",
  "types": "./index.d.ts"
}

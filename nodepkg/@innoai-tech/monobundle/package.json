{
  "name": "@innoai-tech/monobundle",
  "monobundle": {
    "build": {
      "clean": true
    },
    "bin": {
      "monobundle": "./src/bin/index.ts"
    },
    "exports": {
      ".": "./src/index.ts"
    }
  },
  "version": "0.1.0",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "*",
    "@babel/runtime-corejs3": "*",
    "@babel/types": "^7.18.4",
    "@innoai-tech/lodash": "workspace:^*",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "chalk": "^5.0.1",
    "del": "^6.1.1",
    "minimatch": "^5.1.0",
    "rollup": "^2.75.5",
    "rollup-plugin-dts": "^4.2.2",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.10"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "scripts": {
    "prepublishOnly": "../../../node_modules/.bin/monobundle"
  },
  "bin": {
    "monobundle": "./monobundle.mjs"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:innoai-tech/runtime.git",
    "directory": "nodepkg/@innoai-tech/monobundle"
  },
  "type": "module",
  "types": "./index.d.ts"
}
